<odoo>
    <data>
        <record id="seq_student" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">student1.Student.seq</field>
            <field name="prefix">S</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- add sequence with year  -->

        <record id="seq_teacher" model="ir.sequence">
            <field name="name">Teacher Sequence</field>
            <field name="code">student1.teacher.seq</field>
            <field name="prefix">T%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
            <field name="number_next_actual">1</field>
        </record>
        
        
    </data>
</odoo>
